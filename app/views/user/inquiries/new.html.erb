<div class="user_inquiry">
 <h2 id="i_title">お問い合せ</h2>
 <%= form_with model: Inquiry.new do |f| %>
  <ul class="user_inquiry_form">
   <li class="i_label">
    名前
   </li>
   <li class="i_ele">
    <%= f.text_field :name %>
   </li>
   <li class="i_label">
    件名
   </li>
   <li class="i_ele">
    <%= f.text_field :title %>
   </li>
   <li class="i_label">
    お問い合わせ内容
   </li>
   <li class="i_ele">
    <%= f.text_area :content %>
   </li>
   <li class="i_label">
    電話番号
   </li>
   <li class="i_ele">
    <%= f.text_field :phone %>
   </li>
   <li class="i_label">
    メールアドレス
    </li>
    <li class="i_ele">
    <%= f.text_field :mail %>
   </li>
   <li class="submit">
    <%= f.submit "送信" %>
   </li>
  </ul>
 <% end %>
</div>


<%= link_to "メニュー追加", new_admin_menu_path %>
<h1>メニュー一覧</h1>
<% @menus.each do |menu| %>
  <table class="menu">
    <tr>
      <td>
        <%= link_to admin_menu_path(menu.id) do %>
          <%= menu.name %>
        <% end %>
      </td>
      <td>¥<%= menu.price %></td>
    </tr>
  </table>
<% end %>

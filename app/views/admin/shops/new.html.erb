<h2>店舗情報新規画面</h2>
<%= form_with mdoel: @shop, url: "/admin"  do |f| %>
  <dl>
    <dt>店舗名</dt>
    <dd>為ちゃんち</dd>
  </dl>
  <dl>
    <dt>営業時間</dt>
    <dd><%= f.text_field :business_time %></dd>
  </dl>
  <dl>
    <dt>営業日</dt>
    <dd><%= f.text_field :business_time %></dd>
  </dl>
  <dl>
    <dt>定休日</dt>
    <dd><%= f.text_field :holiday %></dd>
  </dl>
  <dl>
    <dt>郵便番号</dt>
    <dd><%= f.text_field :post_code %></dd>
  </dl>
  <dl>
    <dt>住所</dt>
    <dd><%= f.text_field :address %></dd>
  </dl>
  <dl>
    <dt>電話番号</dt>
    <dd><%= f.text_field :telephone %></dd>
  </dl>
  <dl>
    <dd><%= f.submit "送信" %></dd>
  </dl>
<% end %>